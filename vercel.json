{
	"headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, must-revalidate"
        }
      ]
    },
		{
			"source": "/pricing",
			"headers": [
				{
					"key": "Cache-Control",
          "value": "public, max-age=86400, must-revalidate"
				}
			]
		},
		{
			"source": "/(.+\\.png)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}
