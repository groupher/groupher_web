{
  "compilerOptions": {
    "baseUrl": ".",
    "noUnusedLocals": true,
    "paths": {
      "~/containers/*": ["src/containers/*"],
      "~/widgets/*": ["src/widgets/*"],
      "~/services": ["src/services"],
      "~/services/*": ["src/services/*"],
      "~/const": ["utils/constant"],
      "~/const/*": ["utils/constant/*"],
      "~/hooks": ["src/hooks"],
      "~/hooks/*": ["src/hooks/*"],
      "~/hoc": ["src/hoc"],
      "~/config": ["config"],
      "~/stores/*": ["src/stores/*"],
      "~/utils": ["utils"],
      "~/utils/*": ["utils/*"],
      "~/server": ["utils/api"],
      "~/server/*": ["utils/api/*"],
      "~/css": ["utils/css"],
      "~/css/*": ["utils/css/*"],
      "~/schemas": ["src/schemas"],
      "~/Img": ["src/widgets/Img"],
      "~/SvgIcons/*": ["src/widgets/SvgIcons/*"],
      "~/icons/*": ["src/widgets/icons/*"],
      "~/i18n": ["i18n"]
    }
  },
  "exclude": ["node_modules"]
}
